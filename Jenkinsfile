pipeline
{
    agent any
    
    stages{
        stage("Build"){
            steps{
                echo("Building the project")
            }
        }
        stage("Run Smoke Test"){
            steps{
                echo("Run the tests")
            }
        }
        stage("Deploy to QA"){
            steps{
                echo("deploy dev")
            }
        }
        stage("Generate Report"){
            steps{
                echo("Generate the test reports")
            }
        }
    }
    
}


